<!DOCTYPE html>
<html>

<head>
    <title>{{ options.title }}</title>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <meta content="yes" name="mobile-web-app-capable">
    <meta content="kss-node" name="generator">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter&family=Oswald&family=Podkova&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.15.4/css/all.css" integrity="sha384-rqn26AG5Pj86AF4SO72RK5fyefcQ/x32DNQfChxWvbXIyXFePlEktwD18fEz+kQU" crossorigin="anonymous">
    <link href="kss-assets/vendor/prism.css" rel="stylesheet">
    <link href="kss-assets/css/kss.css" rel="stylesheet">
    {{{ styles }}} <style>
        :root {
            /*  write all the css variables that need to be overridden in the design from the admin.  */
            /* Colors are broken into hsla for greater flexibility. */
            --brand-h: 228.8deg;
            --brand-s: 75%;
            --brand-l: 39.2156862745%;
            --primary-h: 205.2857142857deg;
            --primary-s: 58.8235294118%;
            --primary-l: 46.6666666667%;
            --secondary-h: 223.6363636364deg;
            --secondary-s: 5.069124424%;
            --secondary-l: 42.5490196078%;
            --accent-h: 19.9078341014deg;
            --accent-s: 89.3004115226%;
            --accent-l: 47.6470588235%;
            --highlight-h: 270.9677419355deg;
            --highlight-s: 89.3004115226%;
            --highlight-l: 47.6470588235%;
            /* font family rules based on the selected google font. */
            --font_accent: "Podkova", Lucida Bright, Georgia, serif;
            --font_display: "Oswald", Open Sans, Helvetica Neue, Helvetica, Helvetica, Arial, sans-serif;
            --font_copy: "Inter", Helvetica Neue, Helvetica, Helvetica, Arial, sans-serif;
            --font_ui: "Inter", -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Helvetica, Arial, sans-serif;
            --font_mono: SFMono-Regular, Menlo, Monaco, Consolas, Liberation Mono, Courier New, monospace;
            /* reading typography overrides. */
            --font-family_headline: var(--font-family_display);
            --font-family_headline-alt: var(--font-family_accent);
            --font-family_content: var(--font-family_copy);
            --font-family_code: var(--font-family_mono);
            /* Bootstrap Versions */
            --bs-primary: hsl(var(--primary-h), var(--primary-s), var(--primary-l));
            --bs-secondary: hsl(var(--secondary-h), var(--secondary-s), var(--secondary-l));
            --bs-info: hsl(var(--accent-h), var(--accent-s), var(--accent-l));
            ;
            --bs-primary-rgb: 13, 110, 253;
            /* admin area will have to render these as rgb */
            --bs-secondary-rgb: 108, 117, 125;
            /* admin area will have to render these as rgb */
            --bs-info-rgb: 13, 202, 240;
            /* admin area will have to render these as rgb */
            --bs-font-sans-serif: var(--font-family_ui);
            --bs-font-monospace: var(--font-family_mono);
            --bs-gradient: linear-gradient(180deg, hsla(var(--primary-h), var(--primary-s), var(--primary-l), 0.15), hsla(var(--primary-h), var(--primary-s), var(--primary-l), 0));
            --bs-body-font-family: var(--bs-font-sans-serif);
        }
    </style>
</head>

<body class="kss-body  bg_texture-light font_ui bg_black-2">
    <!-- header. -->
    <header class="bg_white shadow_overlap-light br_black-1 br_solid br_1 sticky t_0 display_none:md flex flex_inline z_5">
        <h1 class="self_center m_3">
            <a class="undecorated h:undecorated" href="/WebCasters/index.html">{{ options.title }}</a>
        </h1>
        <a class="m-l_auto self_center m_3 kss-header__hamburger-trigger">
            <div class="kss-header__hamburger"></div>
        </a>
    </header>
    <!-- /header. -->
    <!-- navigation. -->
    <!-- /navigation. -->
    <!-- documentation. -->
    <header class="p_4 text_center">
        <h1 class="font_display font_10 font_light display_none block:md">
            <a class="undecorated h:undecorated c_primary" href="./index.html">{{ options.title }}</a>
        </h1>
    </header>
    <section id="HeaderNav" class="display_none block:md m_auto bg_white shadow_bevel-bold">
        <nav class="m_auto font_copy display_none block:md sticky t_0">
            <ul class="ul_none flex flex_row:md flex_column justify_center br-t_1 br_solid br_black-4 p-y_3 br-b_1 ">
                <li class=" w_auto:md w_100 p_2 self_center {{#if homepage}}is-active active{{/if}}">
                    <a class="block inline-block:md undecorated {{#if isActive}}is-active active{{/if}} h:bg_black-2 a:bg_accent a:c_white h:undecorated c_black-8 h:c_black-9 p_3 br_radius nowrap" href="index.html">
                        <span class=""><i class="fas fa-home p-x_2"></i></span>
                    </a>
                </li>
                {{#each menu}}
                    <li class="w_auto:md w_100 br-l_1 br_black-2 br_solid:md p_2 self_center {{#if isActive}}is-active active{{/if}}">
                        <a class="block inline-block:md c_black-8 h:c_black-9 undecorated {{#if isActive}}is-active active{{/if}} h:bg_black-2 a:bg_accent a:c_white h:undecorated p_3 br_radius nowrap" href="section-{{ referenceURI }}.html">
                            <span class="">{{ header }}</span>
                        </a>
                    </li>
                {{/each}}
            </ul>
        </nav>
    </section>
    <section id="MobleNav" class="bg_secondary-n3 bg_white display_none:lg font_0 font_ui kss-navigation overflow_auto p-b_2 p-x_3 scrollbar-mini shadow_overlap-bold z_4" style="padding-top:70px">
        <nav class="display_none:md flex flex_column-reverse">
            <ul class="block flex flex_column ul_none w_100 ul_none lh_3" id="sitenav">
                {{#each options.site_nav }}
                    <li class="flex_auto {{#if isActive}}is-active active{{/if}} m-b_2">
                        <a class="c_white undecorated h:undecorated a:bg_primary h:bg_black-5 p_2 block br_radius" href="{{ url }}" id="{{ id }}">{{ title }}</a>
                    </li>
                    {{#if isActive}}
                        <li class="flex_auto m-y_2 p_3 bg_black-5 br_radius p_3 shadow_emboss-light">
                            <ul class="block flex flex_column ul_none w_100 ul_none lh_3">
                                <li class="{{#if ../homepage}}is-active active{{/if}} m-b_2 a:bg_accent h:bg_black-5 relative br_radius p_2 m-b_1">
                                    <a class="{{#if ../homepage}}is-active active{{/if}}  a:c_white c_white-8 h:c_white" href="index.html">
                                        <span class=""><i class="fas fa-home p-x_2"></i> Home</span>
                                    </a>
                                </li>
                                {{#each ../menu}}
                                    <li class="{{#if isActive}}is-active active{{/if}} m-b_2 a:bg_accent h:bg_accent-4 relative br_radius p_2 p-l_3 m-b_1">
                                        <a class="{{#if isActive}}is-active active{{/if}} a:c_white c_white-8 h:c_white block inline-block:md undecorated flex flex_row h:undecorated flex_nowrap" href="section-{{ referenceURI }}.html">
                                            <span class="">{{ header }}</span>
                                        </a>
                                    </li>
                                    {{#if isActive}}
                                        {{#if children}}
                                            <ul class="m-l_4 ul_none lh_3">
                                                {{#each children}}
                                                    <li class="{{#if isGrandChild}} p-l_4 font-size_down lh_3 {{else}} font_bold {{/if}} m-b_2">
                                                        <a class="block inline-block:md undecorated flex flex_row h:undecorated flex_nowrap   c_white-8 h:c_white" href="section-{{ ../referenceURI }}.html#kssref-{{ referenceURI }}">
                                                            <span class="kss-nav__ref flex_shrink p-r_3 font_bold">{{ referenceNumber }}</span>
                                                            <span class="flex_auto overflow_ellipsis">{{ header }}</span>
                                                        </a>
                                                    </li>
                                                {{/each}}
                                            </ul>
                                        {{/if}}
                                    {{/if}}
                                {{/each}}
                            </ul>
                        </li>
                    {{/if}}
                {{/each}}
                <li class="flex_auto m-b_2">
                    <a class="c_white undecorated h:undecorated a:bg_primary h:bg_black-5 p_2 block br_radius" href="https://acc-style.github.io/origami6/?path=/story/introduction--page" id="origami"><i class="fas fa-paper-plane p-r_3"></i><span>Project Origami</span></a>
                </li>
            </ul>
        </nav>
    </section>
    <div class="m_auto p-x_4">
        {{#if homepage}}
            <article class="kss-markdown reading-typography font-size_up p_4 p-t_2 flex_auto max-w_90 m_auto">
                {{{ homepage }}}
            </article>
            {{ else }}
            <!-- display each section, in order. the "root" element comes first in this loop, and can be detected using the "#if @first" block as seen below. -->
            <div class="flex flex_column flex_row:md gap-x_5 p-t_4 max-w_90 m_auto">
                <aside class="max-w_20 w_25:lg flex_none relative display_none block:md">
                    <nav class="sticky t_0 br_round shadow_bevel-bold bg_white p_4">
                        {{#each menu}}
                            {{#if isActive}}
                                <h3>{{ header }}</h3>
                                {{#if children}}
                                    <ul class="m-l_0 ul_none lh_2">
                                        {{#each children}}
                                            <li class="{{#if isGrandChild}}p-l_4 font-size_down lh_2{{else}} font_bold {{/if}}">
                                                <a class="block inline-block:md undecorated flex flex_row h:undecorated flex_nowrap" href="section-{{ ../referenceURI }}.html#kssref-{{ referenceURI }}">
                                                    <span class="flex_auto overflow_ellipsis">{{ header }}</span>
                                                </a>
                                            </li>
                                        {{/each}}
                                    </ul>
                                {{/if}}
                            {{/if}}
                        {{/each}}
                        <div class=" font_bold br-t_1 br_primary br_solid font_bold m-t_3 p-t_3">
                            <a class="block inline-block:md undecorated flex flex_row h:undecorated flex_nowrap" href="#topbody">
                                <i class="fas fa-arrow-up"></i><span class="flex_auto overflow_ellipsis">Top</span>
                            </a>
                        </div>
                    </nav>
                </aside>
                <main class="w_100 flex_auto">
                    {{#each sections}}
                        {{#if @first}}
                            <header class="kss-section kss-section--depth-{{ depth }} br_radius shadow_bevel-bold bg_white br_solid br_black-4 br_1" id="kssref-{{ referenceURI }}">
                                {{ else }}
                                <section class="kss-section kss-section--depth-{{ depth }} br_radius shadow_bevel-light bg_white br_solid br_black-4 br_1" id="kssref-{{ referenceURI }}">
                        {{/if}}
                        <article class="kss-section__content relative br_radius">
                            <div class="absolute t_0 r_4 flex flex_row gap-x_2 justify_end">
                                {{#if deprecated}}
                                    <div class="inline-block flex_none p_2 bg_alert shadow_overlap-light">
                                        <i class="fas fa-fw fa-exclamation-triangle"></i>
                                    </div>
                                {{/if}}
                                {{#if experimental}}
                                    <div class="inline-block flex_none p_2 bg_warning shadow_overlap-light">
                                        <i class="fas fa-fw fa-vial"></i>
                                    </div>
                                {{/if}}
                                {{#if note}}
                                    <div class="inline-block flex_none p_2 bg_highlight c_white shadow_overlap-light">
                                        <i class="fas fa-fw fa-comment-alt"></i>
                                    </div>
                                {{/if}}
                                {{#if format}}
                                    <div class="inline-block flex_none p_2 bg_accent-n1 c_white shadow_overlap-light">
                                        <i class="fas fa-fw fa-text-size"></i>
                                    </div>
                                {{/if}}
                                <a class="c_white link-reset bg_black-5 h:c_white flex_none h:bg_black-7 inline-block p_2 shadow_overlap-light" target="_blank" href="item-{{referenceURI}}.html" title="pop out component into new tab">
                                    <i class="fa-external-link fa-fw fas fa-fw fas"></i>
                                </a>
                            </div>
                            {{#if source.filename}}
                                <p class="kss-section__source font_n3"> Source: <span>{{ source.filename }}</span>, line <span>{{ source.line }}</span>
                                </p>
                            {{/if}}
                            <a class="kss-section__permalink" href="#kssref-{{ referenceURI }}">
                                <h{{ depth }} class="kss-section__item c_primary kss-section__item--depth-{{ depth }}">
                                    <span class="kss-section__ref">{{ referenceNumber }}</span>
                                    <span class="kss-section__name">{{ header }}</span>
                                </h{{ depth }}>
                            </a>
                            {{#if description}}
                                <div class="kss-section__description reading-typography font-size_up overflow_auto ">
                                    <div class="font_n1">
                                        {{#if deprecated}}
                                            <span class="bg_alert-n1 c_white m-x_2 inline-block p-x_3 p_2 br_round uppercase">deprecated</span>
                                        {{/if}}
                                        {{#if experimental}}
                                            <span class="bg_warning-n1 c_white m-x_2 inline-block p-x_4 p_2 br_round uppercase">experimental</span>
                                        {{/if}}
                                    </div>
                                    {{{ description }}}
                                </div>
                            {{/if}}
                            {{#if parameters}}
                                <ul class="kss-parameters">
                                    {{#each parameters}}
                                        <li class="kss-parameters__item">
                                            <p class="kss-parameters__description">
                                                {{{ description }}} {{#if defaultValue}} Defaults to: <code>{{ defaultValue }}</code>
                                                {{/if}}
                                            </p>
                                            <code class="kss-parameters__name">{{ name }}</code>
                                        </li>
                                    {{/each}}
                                </ul>
                            {{/if}}
                        </article>
                        {{#if example}}
                            <article class="kss-section__modifiers">
                                <h1 class="kss-modifiers__heading"> Example {{#if modifiers}}s {{/if}}
                                </h1>
                                {{#if modifiers}}
                                    <h2 class="kss-modifiers__name"> Default styling</h2>
                                {{/if}}
                                <div class="kss-modifiers__example">
                                    <div class="{{markupwrapperclasses}}">
                                        {{{ markup }}}
                                    </div>
                                </div>
                                {{#if modifiers}}
                                    <div class="">
                                        {{#each modifiers}}
                                            <article class="kss-modifiers__modifier">
                                                <h2 class="kss-modifiers__name">
                                                    {{ name }}
                                                </h2>
                                                <p class="kss-modifiers__description">
                                                    {{{ description }}}
                                                </p>
                                                <div class="kss-modifiers__example {{../markupwrapperclasses}}">
                                                    {{{ markup }}}
                                                </div>
                                            </article>
                                        {{/each}}
                                    </div>
                                {{/if}}
                            </article>
                        {{/if}}
                        {{#if format}}
                            <div class="bg_accent-4 block c_black inline-block p-y_3 p-x_4 w_100">
                                <h5 class="font_bold m_0 c_black inline-block">FORMAT:</h5>
                                {{{format}}}
                            </div>
                        {{/if}}
                        {{#if note}}
                            <div class="bg_highlight-4 block c_black inline-block p-y_3 p-x_4 w_100">
                                <h5 class="font_bold m_0 c_black">NOTE:</h5>
                                {{{note}}}
                            </div>
                        {{/if}}
                        {{#if example}}
                            <section class="relative {{ hidecodeblock }} br-t_1 br_solid br_black-2 bg_black-1 shadow_n1 texture_light">
                                <main class="kss-section__markup overflow_scroll scrollbar-mini collapse transition_3" id="collapse-{{ referenceURI }}">
                                    <pre><code class="language-markup"  id="code-{{ referenceURI }}" value='{{ markup }}'>{{ markup }}</code></pre>
                                    <input class="display_none text-input html-code-input" type="text" name="" id="input-{{ referenceURI }}" value='{{ markup }}'>
                                </main>
                                <header class="flex_row flex shadow_n1 br-br_radius br-bl_radius">
                                    <div class="flex flex_row flex_auto collapsed h:bg_black-7 br-bl_radius c_black-8 h:c_white" aria-expanded="false" data-bs-toggle="collapse" data-bs-target="#collapse-{{ referenceURI }}" aria-controls="#collapse-{{ referenceURI }}">
                                        <div class="flex_none p_2 font_1">
                                            <i class="a:rotation fa-fw fa-chevron-up fas self_center text_center"></i>
                                        </div>
                                        <div class="flex_auto flex flex_row">
                                            <h4 class="font_0 font_bold flex_auto p-l_2 self_center m_0 text_left">Code Sample</h4>
                                        </div>
                                    </div>
                                    <a class="bg_primary br_1 br_black-3 br_solid c_white flex_none font_medium font_ui h:bg_primary-n2 h:undecorated m-l_auto p-x_3 p-y_2 br-br_radius" onclick="CopyCode( 'input-{{ referenceURI }}')"><i class="fas fa-copy"></i> Copy Code</a>
                                </header>
                            </section>
                        {{/if}}
                        {{#if @first}}
                            </header>
                            {{ else }}
                            </section>
                        {{/if}}
                    {{/each}}
                </main>
            </div>
        {{/if}}
        </section>
        <!-- /documentation. -->
        <!-- scripts. -->
        <script src="kss-assets/vendor/jquery.js"></script>
        <script src="kss-assets/vendor/prism.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
        <script src="kss-assets/js/kss.js"></script>
        <script>
            var CopyCode = function ($id) {
                var copyTextarea = document.getElementById($id);
                console.log($id);
                copyTextarea.classList.remove('display_none');
                copyTextarea.focus();
                copyTextarea.select();

                try {
                    var successful = document.execCommand('copy');
                    var msg = successful ? 'successful' : 'unsuccessful';
                    console.log('Copying text command was ' + msg);
                } catch (err) {
                    console.log('Oops, unable to copy');
                }
                copyTextarea.classList.add('display_none');
            };
        </script>
        {{{ scripts }}}
        <!-- /scripts. -->
</body>

</html>